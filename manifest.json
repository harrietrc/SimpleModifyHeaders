{
  "description": "Serato Firewall Header",
  "manifest_version": 2,
  "name": "serato-firewall-header",
  "version": "1.6.8",
  "homepage_url": "https://github.com/harrietrc/SimpleModifyHeaders",
  "icons": {
    "48": "icons/stop.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": [
      "lib/js-md5/js/md5.min.js",
      "serato-firewall-header.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/stop.png",
    "default_title": "Simple Modify Headers"
  },
  "applications": {
    "gecko": {
      "id": "{95c37502-8b73-4307-a476-95cea21e8c9b}",
      "strict_min_version": "42.0"
    }
  }
}